        <div class="row-fluid">
            <div class="span12">						
                <p>2013 &copy; Atlas Hostels PLC. <a href="{{ path('atlas_privacy_policy') }}">Privacy Policy</a> | <a href="{{ path('atlas_terms') }}">Terms & Conditions</a> | <a href="{{ path('atlas_important_notice') }}">Important Notice</a> | <a href="{{ path('atlas_risk_warning') }}">Risk Warning</a> </p>
            </div>
        </div><!--/row-fluid-->
    </div><!--/container-->	
</div><!--/copyright-->	
<!--=== End Copyright ===-->
{% endblock %}

