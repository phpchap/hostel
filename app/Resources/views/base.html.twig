    <link rel="stylesheet" href="{{ asset('/assets/css/themes/blue.css') }}" id="style_color">
    <link rel="stylesheet" href="{{ asset('/assets/css/themes/headers/header1-blue.css') }}" id="style_color-header-1">    
    <link rel="stylesheet" href="{{ asset('/assets/plugins/fancybox/source/jquery.fancybox.css') }}">           
    <link rel="stylesheet" href="{{ asset('/assets/css/effects.css') }}">           

    {% block before_head %}{% endblock %}
</head> 

<body>

{% block after_body %}
<div id="fb-root"></div>
<script>(function(d, s, id) {
  var js, fjs = d.getElementsByTagName(s)[0];
  if (d.getElementById(id)) return;
  js = d.createElement(s); js.id = id;
  js.src = "//connect.facebook.net/en_GB/all.js#xfbml=1&appId=255995124541477";
  fjs.parentNode.insertBefore(js, fjs);
}(document, 'script', 'facebook-jssdk'));</script>
{% endblock %}

{% block top %}
<!--=== Top ===-->    
<div class="top">
    <div class="container">                 
        <div class="row-fluid">
            <ul class="loginbar inline">
                <li><a href="mailto:enquiries@atlashostelsplc.com"><i class="icon-envelope-alt"></i> enquiries@atlashostelsplc.com</a></li> 
                <li><a><i class="icon-phone-sign"></i>  (+44) 0800 122 3455</a></li>   
            </ul>
        </div>                        
    </div><!--/container-->     
</div><!--/top-->
<!--=== End Top ===-->    
{% endblock %}

{% block header %}
<!--=== Header ===-->
<div class="header">               
    <div class="container"> 
        {% block logo %}
        <!-- Logo -->       
        <div class="logo">                                             
            <a href="/"><img id="logo-header" src="/assets/img/atlashostelsPLC.png" alt="Logo"></a>
        </div><!-- /logo -->                  
        {% endblock %}

        {% block navbar %}
        <!-- Menu -->       
        <div class="navbar">                                
            <div class="navbar-inner">                                  
                <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </a><!-- /nav-collapse -->                                  
                <div class="nav-collapse collapse">                                     
                    <ul class="nav">
                        <li class="active">
                            <a href="{{ path('atlas_homepage') }}">Home</a>
                        </li>
                        <li>
                            <a href="{{ path('atlas_aims_and_objectives') }}">Aims & Objectives</a>
                        </li>
                        <li>
                            <a href="{{ path('atlas_our_operations') }}">Our Operations</a>
                        </li>
                        <li>
                            <a href="{{ path('atlas_contacts') }}">Contacts</a>
                        </li>
                    </ul>
                </div><!-- /nav-collapse -->                                
            </div><!-- /navbar-inner -->
        </div><!-- /navbar -->                          
        {% endblock %}
    </div><!-- /container -->               
</div><!--/header -->      
<!--=== End Header ===-->
{% endblock %}

{% block slider %}{% endblock %}

{% block heading_breadcrumbs %}{% endblock %}

{% block content_part %}{% endblock %}

{% block footer %}
{% endblock %}

